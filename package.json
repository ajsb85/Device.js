{
  "name":           "uupaa.device.js",
  "version":        "0.8.23",
  "description":    "Detect and Query mobile device spec.",
  "url":            "https://github.com/uupaa/Device.js",
  "keywords":       ["mobile", "device", "soc", "query", "spec", "util", "uupaa"],
  "repository": {
    "type":         "git",
    "url":          "https://github.com/uupaa/Device.js.git"
  },
  "scripts": {
    "watch":        "node node_modules/uupaa.watch.js --verbose --action build ./lib/Device.js",
    "build":        "node node_modules/uupaa.minify.js --keep --option 'formatting pretty_print' --output ./lib/Device.min.js ./lib/Device.js",
    "build-release":"node node_modules/uupaa.minify.js --module all --output ./lib/Device.min.js",
    "test":         "NODE_ENV=production NODE_PATH=lib node --harmony test/index.node.js; open test/index.html",
    "lint":         "plato --title Device.js --jshint .jshintrc -d lint/plato lib/Device.js; open lint/plato/index.html"
  },
  "build": {
    "all": {
      "lib/Device.js": "",
      "lib/Android.js": "",
      "lib/WindowsPhone.js": ""
    },
    "node": { },
    "worker": { },
    "browser": { }
  },
  "dependencies": {
    "uupaa.spec.js":    "",
    "uupaa.useragent.js": ""
  },
  "devDependencies": {
    "uupaa.valid.js":   "",
    "uupaa.help.js":    "",
    "uupaa.test.js":    "",
    "uupaa.task.js":    "",
    "uupaa.minify.js":  "",
    "uupaa.spec.js":    "",
    "uupaa.useragent.js": ""
  },
  "lib":            "./lib/",
  "main":           "./index.js",
  "author":         "uupaa <uupaa.js@gmail.com>",
  "license":        "MIT",
  "contributors":   []
}
